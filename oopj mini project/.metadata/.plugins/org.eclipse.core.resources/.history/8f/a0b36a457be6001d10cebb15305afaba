package harshal.fardin.main;

import java.util.Arrays;

public class MissingNumber {
	public static void find(int[] arr) {
		int n = arr.length + 2;
		int sum = n*(n + 1) / 2;
		for (int i = 0; i < arr.length ; i++) {
			sum=sum-arr[i];

		}
		int a=sum/2;
		int b=(sum+1)/2;
		System.out.println();
		System.out.println();
		
	}
	public static void main(String[] args) {
		int[]fardin= {1,4,6,5};
		//Arrays.sort(fardin);
		System.out.println(Arrays.toString(fardin));
		
	}

}

